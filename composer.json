{
    "name": "innmind/http-transport",
    "type": "library",
    "description": "Library to fulfill http resquest",
    "keywords": ["http", "client"],
    "homepage": "http://github.com/Innmind/HttpTransport",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpTransport/issues"
    },
    "require": {
        "php": "~8.0",
        "innmind/http": "dev-next",
        "guzzlehttp/guzzle": "~7.0",
        "psr/log": "^1.0",
        "ramsey/uuid": "~4.0",
        "innmind/time-warp": "dev-next",
        "innmind/time-continuum": "dev-next",
        "innmind/immutable": "dev-next",
        "innmind/url": "dev-next"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Innmind\\HttpTransport\\": "src/"
        },
        "files": ["src/bootstrap.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\HttpTransport\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.4",
        "innmind/coding-standard": "~2.0"
    }
}
