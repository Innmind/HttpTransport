{
    "name": "innmind/http-transport",
    "type": "library",
    "description": "Library to fulfill http resquest",
    "keywords": ["http", "client"],
    "homepage": "http://github.com/Innmind/HttpTransport",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpTransport/issues"
    },
    "require": {
        "php": "~8.1",
        "ext-curl": "*",
        "innmind/http": "~5.2",
        "innmind/filesystem": "~5.2|~6.0",
        "psr/log": "~3.0",
        "ramsey/uuid": "^4.7",
        "innmind/time-warp": "~3.0",
        "innmind/time-continuum": "~3.0",
        "innmind/immutable": "~4.1",
        "innmind/url": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\HttpTransport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\HttpTransport\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.30",
        "innmind/coding-standard": "~2.0",
        "innmind/black-box": "^4.18"
    }
}
